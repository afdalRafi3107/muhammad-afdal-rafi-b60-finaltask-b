{{#> layout currentPage='addKab'}}


    <!-- content -->
    <div class="container-md container-input" style="text-align: center;">
        <h1>Add Kabupaten</h1>
        <form id="tambahKab" action="/addKab" method="post" enctype="multipart/form-data">
        <div class="input-group mb-3">
         <input name="nama" type="text" class="form-control" placeholder="Nama Kabupaten/kota" aria-label="Recipient's username" aria-describedby="basic-addon2">
        </div>
        <div class="input-group mb-3" style="display: none">
         <input name="provinsi_id" type="text" class="form-control" aria-label="Recipient's username" aria-describedby="basic-addon2" value="{{prov.id}}">
        </div>
        <div class="input-group mb-3">
         <span class="input-group-text" id="basic-addon2">Diresmikan</span>
         <input name="diresmikan" type="date" class="form-control" placeholder="Diresmikan" aria-label="Recipient's username" aria-describedby="basic-addon2">
        </div>
        <div class="input-group mb-3">
         <input name="image" type="file" class="form-control" placeholder="Logo" aria-label="Recipient's username" aria-describedby="basic-addon2">
        </div>
        <div class="input-group mb-3" >
        <button class="btn btn-success" style="width: 100%;" type="submit">Submit</button>
        </div>
        </form>
    </div>
 {{/layout }}